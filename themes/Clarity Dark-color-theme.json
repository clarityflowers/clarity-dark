{
  "name": "Clarity Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "JSX",
      "scope": ["source.js meta.tag"],
      "settings": {
        "foreground": "#62A9FF"
      }
    },
    {
      "name": "css-in-js",
      "scope": ["source.js source.css"],
      "settings": {
        "foreground": "#62A9FF"
      }
    },
    {
      "name": "JSX embedded",
      "scope": ["source.js meta.tag meta.embedded.expression.js"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF4848"
      }
    },
    {
      "name": "Documentation",
      "scope": [
        "comment.block.documentation",
        "comment.block.documentation punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#2F74D0"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#FF4848"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant",
        "string",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#FF97E8"
      }
    },
    {
      "name": "css-in-js constants",
      "scope": [
        "source.js source.css constant.numeric",
        "source.js source.css constant.character",
        "source.js source.css constant",
        "source.js source.css string",
        "source.js source.css constant.language",
        "source.js source.css support.constant"
      ],
      "settings": {
        "foreground": "#FF97E8"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FF68DD"
      }
    },
    {
      "name": "Template Strings",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "CSS Attribute Names",
      "scope": ["source.css entity.other.attribute-name"],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
